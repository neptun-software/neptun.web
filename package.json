    "shiki-stream": "^0.1.1",
    "strip-markdown": "^6.0.0",
    "tailwind-merge": "^2.6.0",
    "uploadthing": "^7.4.4",
    "vaul-vue": "^0.2.1",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-dndrop": "~1.2.0",
    "vue-router": "^4.5.0",
    "vue-sonner": "^1.3.0",
